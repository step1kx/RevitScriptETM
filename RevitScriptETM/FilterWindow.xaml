<Window x:Class="RevitScriptETM.FilterWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RevitScriptETM"
             mc:Ignorable="d"
             Height="500" Width="800" Title="Фильтр задания">
    <Grid Width="800" Height="500">

        <Button Content="Применить" Width="100" Click="ApplyButton_Click" HorizontalAlignment="Right" Height="30" Margin="0,0,10,30" VerticalAlignment="Bottom"/>
        <Button Content="Отмена" Width="100"  Click="CancelButton_Click" HorizontalAlignment="Left" Height="30" Margin="10,0,0,30" VerticalAlignment="Bottom"/>


    </Grid>
</Window>
